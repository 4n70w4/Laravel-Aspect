{
  "name": "ytake/laravel-aop",
  "description": "aop library for laravel framework, and lumen",
  "minimum-stability": "stable",
  "license": "MIT",
  "keywords": [
    "laravel",
    "aop",
    "aspect"
  ],
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "goaop/framework": "~0.0",
    "illuminate/console": "~5.0",
    "illuminate/filesystem": "~5.0",
    "illuminate/support": "~5.0",
    "doctrine/annotations": "~1.0"
  },
  "require-dev": {
    "symfony/framework-bundle": "2.*",
    "symfony/console": "2.*",
    "phpunit/phpunit": "4.*",
    "ytake/lom": "~0.0"
  },
  "autoload": {
    "psr-4": {
      "Ytake\\LaravelAop\\": "src"
    }
  },
  "Suggests": {
    "ext-aop": "for extension driver"
  }
}
